<?xml version="1.0" encoding="utf-8"?>

<forms:ContentPageBase xmlns="http://xamarin.com/schemas/2014/forms"
                       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                       xmlns:forms="clr-namespace:SocialQ.Forms;assembly=SocialQ.Forms"
                       xmlns:queue="clr-namespace:SocialQ.ViewModels.Queue;assembly=SocialQ.ViewModels"
                       xmlns:queue1="clr-namespace:SocialQ.Forms.Queue;assembly=SocialQ.Forms"
                       x:Class="SocialQ.Forms.Queue.Queues"
                       x:TypeArguments="queue:QueuesViewModel"
                       x:DataType="queue:QueuesViewModel">
    <ContentPage.Content>
        <StackLayout>
            <CollectionView 
                AutomationId="CoffeeListView"
                x:Name="Queue"
                SelectionMode="Single">
                <CollectionView.ItemTemplate>
                    <DataTemplate>
                        <queue1:QueuedItemView />
                    </DataTemplate>
                </CollectionView.ItemTemplate>
            </CollectionView>
            <Button x:Name="Button" />
        </StackLayout>
    </ContentPage.Content>
</forms:ContentPageBase>